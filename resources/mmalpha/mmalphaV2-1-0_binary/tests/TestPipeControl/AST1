    domain[2, {"t", "p"}, {pol[4, 4, 0, 0, 
       {{1, 1, -1, 0}, {1, -1, 1, 3}, {1, 0, 1, 0}, {1, 0, -1, 3}}, 
       {{1, 3, 0, 1}, {1, 0, 0, 1}, {1, 3, 3, 1}, {1, 6, 3, 1}}]}]], 
