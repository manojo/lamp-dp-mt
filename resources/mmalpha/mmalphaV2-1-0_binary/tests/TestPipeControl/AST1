         restrict[domain[2, {"t", "p"}, 
           {pol[4, 4, 0, 0, {{1, 1, -1, 0}, {1, -1, 1, 3}, {1, 0, 1, 0}, 
              {1, 0, -1, 3}}, {{1, 3, 0, 1}, {1, 0, 0, 1}, {1, 3, 3, 1}, 
              {1, 6, 3, 1}}]}], 
          if[unop[not, var["loadC"]], 
           binop[add, affine[var["C"], 
             matrix[3, 3, {"t", "p"}, {{1, 0, -1}, {0, 1, 0}, {0, 0, 1}}]], 
            binop[mul, var["A"], var["B1"]]], 
           affine[const[False], matrix[1, 3, {"t", "p"}, {{0, 0, 1}}]]]]}]]}]
], equation["loadC", case[{restrict[domain[2, {"t", "p"}, 
