system testSchedule2 :{N,M | 1 <= N; 1<=M }
(x: {i| 1<= i <= M}Êof integer)
returns (y: {i| 1<= i <= M} of integer);
var
  A : {i | 1<=i<=M} of integer;
  B : {i,j| 1<=i<=M; 0<=j<=N} of integer;
let
  A[i] = case 
           {|i=1}: x[i]; 
           {|i>1}: x[i] + B[i-1,N]; 
         esac;
  B[i,j] = 
     case
       {|j=1}:  0[];
       {|j>=1}: B[i,j-1]+x[i];
     esac;
  y[i] = A[i];
tel;
