Cell[CellGroupData[{

Cell[TextData[{
  "The ",
  StyleBox["scheduleConstraint", "Input"],
  " function"
}], "Subsection"],

Cell["\<\
This function computes the dependence constraints, and the \
positivity constraints, either mono- or multidimensional, with various \
options. \
\>", "Text",
  TextJustification->1],

Cell[CellGroupData[{

Cell[BoxData[
    \(\(?scheduleConstraints\)\)], "Input"],

Cell["Alpha`VertexSchedule`scheduleConstraints", "Print",
  CellTags->"Info3407929681-2992127"],

Cell[BoxData[
    InterpretationBox[GridBox[{
          {GridBox[{
                {\(scheduleConstraints[
                      Alpha`VertexSchedule`Private`options___Rule] := 
                    scheduleConstraints[$result, {}, 
                      Alpha`VertexSchedule`Private`options]\)},
                {" "},
                {\(scheduleConstraints[
                      Alpha`VertexSchedule`Private`sys_system, 
                      Alpha`VertexSchedule`Private`options___Rule] := 
                    scheduleConstraints[Alpha`VertexSchedule`Private`sys, {}, 
                      Alpha`VertexSchedule`Private`options]\)},
                {" "},
                {\(scheduleConstraints[
                      Alpha`VertexSchedule`Private`variable_String, 
                      Alpha`VertexSchedule`Private`options___Rule] := 
                    scheduleConstraints[$result, 
                      Alpha`VertexSchedule`Private`variable, 
                      Alpha`VertexSchedule`Private`options]\)},
                {" "},
                {\(scheduleConstraints[
                      Alpha`VertexSchedule`Private`sys_system, 
                      Alpha`VertexSchedule`Private`variable : _String | {}, 
                      Alpha`VertexSchedule`Private`options___Rule] := 
                    Catch[Module[{Alpha`VertexSchedule`Private`v, 
                          Alpha`VertexSchedule`Private`lv, 
                          Alpha`VertexSchedule`Private`iv, 
                          Alpha`VertexSchedule`Private`ov, 
                          Alpha`VertexSchedule`Private`declarat, 
                          Alpha`VertexSchedule`Private`dims, 
                          Alpha`VertexSchedule`Private`coeffstau, 
                          Alpha`VertexSchedule`Private`coeffsalpha, 
                          Alpha`VertexSchedule`Private`defs, 
                          Alpha`VertexSchedule`Private`aux, 
                          Alpha`VertexSchedule`Private`result, 
                          Alpha`VertexSchedule`Private`result1, 
                          Alpha`VertexSchedule`Private`pos, 
                          Alpha`VertexSchedule`Private`oploc, 
                          Alpha`VertexSchedule`Private`slp, 
                          Alpha`VertexSchedule`Private`vrb, 
                          Alpha`VertexSchedule`Private`extrac, 
                          Alpha`VertexSchedule`Private`multidim, 
                          Alpha`VertexSchedule`Private`bd, 
                          Alpha`VertexSchedule`Private`vpc, 
                          Alpha`VertexSchedule`Private`dponly, 
                          Alpha`VertexSchedule`Private`dbg, 
                          Alpha`VertexSchedule`Private`depcomp, 
                          Alpha`VertexSchedule`Private`sbs, 
                          Alpha`VertexSchedule`Private`useConstraints, 
                          Alpha`VertexSchedule`Private`df, 
                          Alpha`VertexSchedule`Private`onlyV, 
                          Alpha`VertexSchedule`Private`onlyE, 
                          Alpha`VertexSchedule`Private`onlyD, 
                          Alpha`VertexSchedule`Private`reuseS}, \
$dependencyConstraints = {}; 
                        If[Alpha`VertexSchedule`Private`sys === {}, 
                          Throw[Message[
                              Alpha`VertexSchedule`Private`\
scheduleConstraint::"emptyprog"]]]; 
                        If[\(! normalizeQ[
                              Alpha`VertexSchedule`Private`sys]\), 
                          Throw[Message[
                              Alpha`VertexSchedule`Private`\
scheduleConstraint::"norm"]]]; 
                        Alpha`VertexSchedule`Private`sbs = \(subSystems \
/. \[InvisibleSpace]{Alpha`VertexSchedule`Private`options}\) \
/. \[InvisibleSpace]Options[scheduleConstraints]; 
                        Alpha`VertexSchedule`Private`vpc = \(verticesPositive \
/. \[InvisibleSpace]{Alpha`VertexSchedule`Private`options}\) \
/. \[InvisibleSpace]Options[scheduleConstraints]; 
                        Alpha`VertexSchedule`Private`dponly = \
\(dependenciesOnly /. \[InvisibleSpace]{Alpha`VertexSchedule`Private`options}\
\) /. \[InvisibleSpace]Options[scheduleConstraints]; 
                        Alpha`VertexSchedule`Private`slp = \(scheduleType \
/. \[InvisibleSpace]{Alpha`VertexSchedule`Private`options}\) \
/. \[InvisibleSpace]Options[scheduleConstraints]; 
                        Alpha`VertexSchedule`Private`vrb = \(verbose \
/. \[InvisibleSpace]{Alpha`VertexSchedule`Private`options}\) \
/. \[InvisibleSpace]Options[scheduleConstraints]; 
                        Alpha`VertexSchedule`Private`df = \(dataFlow \
/. \[InvisibleSpace]{Alpha`VertexSchedule`Private`options}\) \
/. \[InvisibleSpace]Options[scheduleConstraints]; 
                        Alpha`VertexSchedule`Private`multidim = 
                          If[Alpha`VertexSchedule`Private`df, 
                            True, \(multidimensional \
/. \[InvisibleSpace]{Alpha`VertexSchedule`Private`options}\) \
/. \[InvisibleSpace]Options[scheduleConstraints]]; 
                        Alpha`VertexSchedule`Private`dbg = \(debug \
/. \[InvisibleSpace]{Alpha`VertexSchedule`Private`options}\) \
/. \[InvisibleSpace]Options[scheduleConstraints]; 
                        Alpha`VertexSchedule`Private`depcomp = \(depComputed \
/. \[InvisibleSpace]{Alpha`VertexSchedule`Private`options}\) \
/. \[InvisibleSpace]Options[scheduleConstraints]; 
                        Alpha`VertexSchedule`Private`dur = \(durations \
/. \[InvisibleSpace]{Alpha`VertexSchedule`Private`options}\) \
/. \[InvisibleSpace]Options[scheduleConstraints]; 
                        Alpha`VertexSchedule`Private`onlyV = \(onlyVars \
/. \[InvisibleSpace]{Alpha`VertexSchedule`Private`options}\) \
/. \[InvisibleSpace]Options[scheduleConstraints]; 
                        Alpha`VertexSchedule`Private`onlyE = \(onlyEquations \
/. \[InvisibleSpace]{Alpha`VertexSchedule`Private`options}\) \
/. \[InvisibleSpace]Options[scheduleConstraints]; 
                        If[Alpha`VertexSchedule`Private`onlyE =!= all, 
                          Print["Warning: option onlyEquations in not \
implemented"]]; 
                        Alpha`VertexSchedule`Private`onlyD = \(onlyDep \
/. \[InvisibleSpace]{Alpha`VertexSchedule`Private`options}\) \
/. \[InvisibleSpace]Options[scheduleConstraints]; 
                        Alpha`VertexSchedule`Private`reuseS = \(reuseSchedule \
/. \[InvisibleSpace]{Alpha`VertexSchedule`Private`options}\) \
/. \[InvisibleSpace]Options[scheduleConstraints]; 
                        Alpha`VertexSchedule`Private`lv = getLocalVars[]; 
                        If[Alpha`VertexSchedule`Private`onlyV =!= all, 
                          Alpha`VertexSchedule`Private`lv = 
                            Alpha`VertexSchedule`Private`lv \[Intersection] 
                              Alpha`VertexSchedule`Private`onlyV]; 
                        Alpha`VertexSchedule`Private`iv = getInputVars[]; 
                        If[Alpha`VertexSchedule`Private`onlyV =!= all, 
                          Alpha`VertexSchedule`Private`iv = 
                            Alpha`VertexSchedule`Private`iv \[Intersection] 
                              Alpha`VertexSchedule`Private`onlyV]; 
                        Alpha`VertexSchedule`Private`ov = getOutputVars[]; 
                        If[Alpha`VertexSchedule`Private`onlyV =!= all, 
                          Alpha`VertexSchedule`Private`ov = 
                            Alpha`VertexSchedule`Private`ov \[Intersection] 
                              Alpha`VertexSchedule`Private`onlyV]; 
                        Alpha`VertexSchedule`Private`v = 
                          Alpha`VertexSchedule`Private`lv \[Union] 
                            Alpha`VertexSchedule`Private`iv \[Union] 
                            Alpha`VertexSchedule`Private`ov; 
                        If[Alpha`VertexSchedule`Private`variable =!= {}, 
                          If[\(! MemberQ[Alpha`VertexSchedule`Private`v, 
                                Alpha`VertexSchedule`Private`variable]\), 
                            Throw[Message[scheduleConstraints::"wrgvar", 
                                Alpha`VertexSchedule`Private`variable]]]]; 
                        Alpha`VertexSchedule`Private`dv = 
                          Alpha`VertexSchedule`Private`lv \[Union] 
                            Alpha`VertexSchedule`Private`ov; 
                        Alpha`VertexSchedule`Private`declarat = 
                          getDeclaration /@ Alpha`VertexSchedule`Private`v; 
                        Alpha`VertexSchedule`Private`domains = \((#1\
\[LeftDoubleBracket]3\[RightDoubleBracket] &)\) /@ 
                            Alpha`VertexSchedule`Private`declarat; 
                        Alpha`VertexSchedule`Private`dims = \((First[#1] &)\) \
/@ Alpha`VertexSchedule`Private`domains; 
                        Alpha`VertexSchedule`Private`coeffstau = 
                          MapThread[
                            Table[ToExpression[
                                  "T" <> #2 <> "D" <> 
                                    ToString[
                                      Alpha`VertexSchedule`Private`i]], \
{Alpha`VertexSchedule`Private`i, 
                                  1, #1}] &, \
{Alpha`VertexSchedule`Private`dims, Alpha`VertexSchedule`Private`v}]; 
                        If[Alpha`VertexSchedule`Private`slp === 
                            sameLinearPart, 
                          Alpha`VertexSchedule`Private`coeffstau = 
                            MapThread[
                              If[MemberQ[
                                    Alpha`VertexSchedule`Private`lv, #2], 
                                  Table[
                                    ToExpression[
                                      "TD" <> 
                                        ToString[
                                        Alpha`VertexSchedule`Private`i]], \
{Alpha`VertexSchedule`Private`i, 
                                      1, #3}], #1] &, \
{Alpha`VertexSchedule`Private`coeffstau, Alpha`VertexSchedule`Private`v, 
                                Alpha`VertexSchedule`Private`dims}]]; 
                        Alpha`VertexSchedule`Private`coeffsalpha = \
\((ToExpression["A" <> #1] &)\) /@ Alpha`VertexSchedule`Private`v; 
                        If[\(! Alpha`VertexSchedule`Private`depcomp\), 
                          If[Alpha`VertexSchedule`Private`dbg, 
                            Print["Computing dependency Table"]]; \
$scheduleDepTable = Check[dep[Alpha`VertexSchedule`Private`sys, 
                                Alpha`VertexSchedule`Private`options, 
                                parameterConstraints \[Rule] True, 
                                subSystems \[Rule] True], 
