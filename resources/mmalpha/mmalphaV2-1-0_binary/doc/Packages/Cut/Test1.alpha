system ss (x: integer) returns (y: integer);
var A, B, C, X1, X, Y : integer;
    Z : {i|} of integer;
    bool1,bool2: boolean;
let
  bool1 = bool1 and bool2;
  bool2 = not bool2;
  bool2 = bool1 or bool2;
  bool2 = bool1 xor bool2;
  X1 = A + (B - C);
  A = -B;
  Y = X*X + (x - X);
  Z = case 
   {i| i=0}:X.(i->); 
   {i| i>0}:-B.(i->);
   {i| i<0}:Z.(i->i+1);
  esac;
tel;
